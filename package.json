{
	"name": "kirby-react",
	"version": "1.0.0",
	"description": "Basic Kirby API + React setup",
	"main": "index.js",
	"scripts": {
		"test": "babel-node src/js/tests/index.js",
		"dev": "npm-run-all --parallel dev:css livereload serve server",
		"dev:js": "webpack --progress --colors",
		"dev:css": "./node_modules/.bin/postcss src/css/*.css --dir assets/css/ -c postcss.config.js -w",
		"build": "npm-run-all build:js build:css",
		"build:js": "webpack -p --config webpack.config.production.js --progress --colors",
		"build:css": "./node_modules/.bin/postcss src/css/*.css --dir assets/css/ -c postcss.config.js",
		"livereload": "livereload 'assets'",
		"serve": "webpack-dev-server",
		"server": "php -S 127.0.0.1:8080"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"html-react-parser": "^0.4.3",
		"js-cookie": "^2.2.0",
		"jsx-transform": "^2.4.0",
		"markdown": "^0.5.0",
		"ramda": "^0.25.0",
		"react": "^16.3.1",
		"react-dom": "^16.3.1",
		"react-router": "^4.2.0",
		"react-router-dom": "^4.2.2",
		"seedrandom": "^2.4.3"
	},
	"devDependencies": {
		"babel-core": "^6.26.0",
		"babel-eslint": "^8.2.2",
		"babel-loader": "^7.1.4",
		"babel-plugin-ramda": "^1.5.0",
		"babel-preset-env": "^1.6.1",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-2": "^6.24.1",
		"css-loader": "^0.28.11",
		"cssnano": "^3.10.0",
		"eslint": "^4.19.1",
		"eslint-config-airbnb": "^16.1.0",
		"eslint-plugin-class-property": "^1.1.0",
		"eslint-plugin-import": "^2.10.0",
		"eslint-plugin-jsx-a11y": "^6.0.3",
		"eslint-plugin-react": "^7.7.0",
		"file-loader": "^1.1.11",
		"node-livereload": "^0.6.0",
		"npm-run-all": "^4.1.2",
		"postcss": "^6.0.21",
		"postcss-advanced-variables": "^2.3.3",

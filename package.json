{
  "name": "kirby-react",
  "version": "1.0.0",
  "description": "Basic Kirby API + React setup",
  "main": "index.js",
  "scripts": {
    "test": "babel-node src/js/tests/index.js",
    "dev": "npm-run-all --parallel dev:js dev:css livereload serve",
    "dev:js": "webpack --progress --colors --watch",
    "dev:css": "./node_modules/.bin/postcss src/css/*.css --dir assets/css/ -c postcss.config.js -w",
    "build": "npm-run-all build:js build:css",
    "build:js": "webpack -p --progress --colors",
    "build:css": "./node_modules/.bin/postcss src/css/*.css --dir assets/css/ -c postcss.config.js",
    "livereload": "livereload 'assets'",
    "serve": "php -S 127.0.0.1:8080"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "axios": "^0.16.2",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.4",
    "cssnano": "^3.10.0",
    "file-loader": "^0.11.2",
    "html-react-parser": "^0.3.5",
    "js-cookie": "^2.1.4",
    "jsx-transform": "^2.3.0",
    "live-reload": "^1.1.0",
    "markdown": "^0.5.0",
    "npm-run-all": "^4.0.2",
    "postcss": "^6.0.2",
    "postcss-advanced-variables": "^1.2.2",
    "postcss-cli": "^4.1.0",
    "postcss-cssnext": "^2.11.0",
    "postcss-extend": "^1.0.5",
    "postcss-filter-stream": "0.0.6",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-math": "0.0.8",
    "postcss-mixins": "^6.0.1",
    "postcss-nested": "^2.0.2",
    "precss": "^1.4.0",
    "prop-types": "^15.5.10",
    "ramda": "^0.24.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "redux": "^3.7.1",
    "seedrandom": "^2.4.3",
    "uglify-js": "^3.0.19",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.0.0"
  }
}
